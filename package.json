{
  "name": "apidocs",
  "version": "0.1.0",
  "private": true,
  "homepage": "api-catalog",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "REACT_APP_ENV=production REACT_APP_PENDO_ENABLED=true REACT_APP_PENDO_API_KEY=4c10d138-7686-497b-5938-3a40b35ac4aa next build",
    "prettier": "prettier --write \"**/*.tsx\"",
    "dev": "concurrently \"node proxy.mjs\"  \"DEV=true next dev\"",
    "lint": "eslint --ext .js,.ts,.tsx ./src",
    "start": "next start",
    "circular": "madge --circular ./src --extensions js,ts,tsx",
    "test": "npm exec jest --src discovery",
    "clean": "npm run clean --workspaces --if-present",
    "discovery": "npm-run-all discovery:build discovery:start",
    "discovery:build": "npm run --workspace packages/common --workspace packages/transform build",
    "discovery:start": "npm run --workspace packages/transform start -- --input-dir `npm prefix`/packages/discovery -o `npm prefix`/packages/common/config",
    "sitemap": "npm-run-all sitemap:build sitemap:start",
    "sitemap:build": "npm run --workspace packages/common -workspace packages/sitemap build",
    "sitemap:start": "npm run --workspace packages/sitemap start --  -o `npm prefix`/public -b $SITEMAP_BASE_URL"
  },
  "dependencies": {
    "@patternfly/patternfly": "^5.4.2",
    "@patternfly/react-code-editor": "^5.4.18",
    "@patternfly/react-core": "^5.4.14",
    "@patternfly/react-icons": "^5.4.2",
    "@patternfly/react-styles": "^5.4.1",
    "@patternfly/react-table": "^5.4.16",
    "@patternfly/react-tokens": "^5.4.1",
    "axios": "^1.12.2",
    "eslint-config-next": "^15.5.6",
    "httpsnippet-lite": "^3.0.5",
    "immer": "^9.0.21",
    "lodash.snakecase": "^4.1.1",
    "lodash.startcase": "^4.4.0",
    "mock-json-schema": "^1.1.2",
    "next": "^15.5.6",
    "prettier": "^3.6.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^8.0.7",
    "react-use": "^17.6.0",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@apidevtools/swagger-parser": "^10.1.1",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.38.0",
    "@rollup/plugin-commonjs": "^24.1.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.3.1",
    "@rollup/plugin-typescript": "^11.1.6",
    "@spaship/cli": "^0.14.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "^27.5.2",
    "@types/lodash.snakecase": "^4.1.9",
    "@types/lodash.startcase": "^4.4.9",
    "@types/node": "^20.19.23",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "@types/swagger2openapi": "^7.0.4",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "ajv": "^8.17.1",
    "assert-never": "^1.4.0",
    "better-ajv-errors": "^1.2.0",
    "concurrently": "^9.2.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "gh-pages": "^5.0.0",
    "glob": "^9.3.5",
    "monaco-editor-webpack-plugin": "^7.1.1",
    "npm-run-all2": "^5.0.2",
    "openapi-types": "^12.1.3",
    "read-json-value": "^1.0.2",
    "rimraf": "^4.4.1",
    "rollup": "^2.79.2",
    "sass": "^1.93.2",
    "sitemaps": "^2.0.6",
    "swagger2openapi": "^7.0.8",
    "ts-jest": "^29.4.5",
    "typescript": "^4.9.5",
    "typescript-eslint": "^8.46.2",
    "yaml": "^2.8.1"
  },
  "jest": {
    "transform": {
      "^.+\\.[t|j]sx?$": "ts-jest"
    }
  }
}
