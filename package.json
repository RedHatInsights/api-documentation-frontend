{
  "name": "apidocs",
  "version": "0.1.0",
  "private": true,
  "homepage": "apidocs",
  "scripts": {
    "build": "react-app-rewired build",
    "eject": "react-app-rewired eject",
    "start": "react-app-rewired start",
    "test": "react-scripts test",
    "discovery": "npm-run-all discovery:build discovery:clean discovery:start",
    "discovery:build": "npm run --prefix transform build",
    "discovery:clean": "rm -rf src/config/apis",
    "discovery:start": "npm run --prefix transform start -- --discovery-file `npm prefix`/discovery/Discovery.yml -o `npm prefix`/src/config"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "dependencies": {
    "@patternfly/patternfly": "^4.222.4",
    "@patternfly/react-code-editor": "^4.82.113",
    "@patternfly/react-core": "^4.267.6",
    "@patternfly/react-icons": "^4.93.3",
    "@patternfly/react-styles": "^4.92.3",
    "@patternfly/react-table": "^4.112.6",
    "@patternfly/react-tokens": "^4.94.3",
    "@spaship/cli": "^1.5.5",
    "@types/dot": "^1.1.5",
    "dot": "^1.1.3",
    "mock-json-schema": "^1.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.8.0",
    "react-router-dom": "^6.8.0",
    "react-scripts": "5.0.1",
    "react-use": "^17.4.0",
    "sass": "^1.57.1",
    "sass-loader": "^12.6.0",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@apidevtools/swagger-parser": "^10.1.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.11",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "ajv": "^8.12.0",
    "gh-pages": "^5.0.0",
    "monaco-editor-webpack-plugin": "^7.0.1",
    "npm-run-all": "^4.1.5",
    "openapi-types": "^12.1.0",
    "react-app-rewired": "^2.2.1",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.4",
    "yaml": "^2.2.1"
  },
  "jest": {
    "transform": {
      "^.+\\.[t|j]sx?$": "ts-jest"
    }
  }
}
